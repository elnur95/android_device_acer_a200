<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

	<PreferenceCategory
	    android:title="@string/category_screen_title">
	    <!-- Touch Sensitivity -->
	    <ListPreference
	        android:key="touch_sensitivity"
	        android:title="@string/touch_sensitivity_head"
	        android:summary="%s"
	        android:entries="@array/touch_sensitivity_entries"
	        android:entryValues="@array/touch_sensitivity_entries_values"
	        android:defaultValue="1" />
	</PreferenceCategory>

</PreferenceScreen>
